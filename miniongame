def minion_game(string):
    length=len(string)
    vow=0
    con=0
    for i in range(length):
        if(string[i] in 'AEIOU'):
            vow=vow+(length-i)
        else:
            con=con+(length-i)
    if(con>vow):

        print("Stuart {}".format(con))

    elif(con==vow):

        print("Draw")
    else:
        print("kevin {}", format (vow))
